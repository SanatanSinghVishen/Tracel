# Dashboard environment variables (Vite)
# Copy to .env.local (recommended) or .env

# Socket.IO backend URL
VITE_SERVER_URL=http://localhost:3001

# Clerk (Google OAuth configured in Clerk dashboard)
VITE_CLERK_PUBLISHABLE_KEY=pk_...

# Optional: override where Clerk loads its JS bundle from.
# Useful if your browser/network blocks *.clerk.accounts.dev.
# Example:
# VITE_CLERK_JS_URL=https://cdn.jsdelivr.net/npm/@clerk/clerk-js@5/dist/clerk.browser.js
VITE_CLERK_JS_URL=

# Simple RBAC (Dashboard "Simulate Attack" button)
# Must match server ADMIN_EMAIL if you want admin features to work end-to-end.
VITE_ADMIN_EMAIL=admin@example.com
